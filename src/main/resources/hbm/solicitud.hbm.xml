<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

    <class name="hbm.Solicitud" table="solicitudes">
        <id name="id" column="id_solicitud">
            <generator class="increment"/>
        </id>

        <property name="cedula">
            <column name="cedula"/>
        </property>

        <property name="paquete">
            <column name="paquete"/>
        </property>

        <property name="fecha">
            <column name="fecha"/>
        </property>

        <property name="comentario">
            <column name="comentario"/>
        </property>

        <property name="aprobado">
            <column name="aprobado"/>
        </property>
    </class>

</hibernate-mapping>
