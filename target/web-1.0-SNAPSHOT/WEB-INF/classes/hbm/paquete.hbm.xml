<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

    <class name="hbm.Paquete" table="paquetes">
        <id name="id" column="id_paquete">
            <generator class="increment"/>
        </id>

        <property name="tarifa">
            <column name="tarifa"/>
        </property>

        <property name="rawDesc">
            <column name="rawDesc"/>
        </property>

        <property name="desde" type="float">
            <column name="desde"/>
        </property>

        <property name="hasta" type="float">
            <column name="hasta"/>
        </property>

        <property name="habilitado">
            <column name="habilitado"/>
        </property>
    </class>

</hibernate-mapping>
